    <sys:String x:Key="SearchButtonText">Поиск</sys:String>
    <sys:String x:Key="SearchBoxText">Поиск по</sys:String>
    <sys:String x:Key="SaveButtonText">Сохранить</sys:String>
    <sys:String x:Key="SaveButtonCheckedText">Сохранено</sys:String>
    <sys:String x:Key="CopyButtonText">Копировать</sys:String>
    <sys:String x:Key="CopyButtonCheckedText">Скопировано</sys:String>
    <sys:String x:Key="WarningMessageText">Редактирование недоступно при подключенном соединении</sys:String>
    
    <!-- Context menus -->
    <sys:String x:Key="RenameMenuItemText">Переименовать</sys:String>
    <sys:String x:Key="DeleteMenuItemText">Удалить</sys:String>
    <sys:String x:Key="PasteMenuItemText">Вставить</sys:String>
    <sys:String x:Key="SelectAllMenuItemText">Выбрать все</sys:String>
    <sys:String x:Key="CutMenuItemText">Вырезать</sys:String>
    <sys:String x:Key="OpenMenuItemText">Открыть</sys:String>
    <sys:String x:Key="ExitMenuItemText">Выход</sys:String>
    
    <!-- ToolTips -->
    <sys:String x:Key="SettingsToolTipText">Настройки</sys:String>
    <sys:String x:Key="HideButtonShowToolTipText">Показать панель</sys:String>
    <sys:String x:Key="HideButtonHideToolTipText">Свернуть панель</sys:String>
    <sys:String x:Key="AddButtonToolTipText">Добавить файл стратегии</sys:String>
    <sys:String x:Key="CloseButtonToolTipText">Закрыть</sys:String>
    <sys:String x:Key="DownButtonToolTipText">Вниз</sys:String>
    <sys:String x:Key="UpButtonToolTipText">Вверх</sys:String>
    <sys:String x:Key="MatchesToolTipText">Совпадения</sys:String>
    <sys:String x:Key="SelectButtonToolTipText">Выбрать</sys:String>
    
    <!-- Other -->
    <sys:String x:Key="SelectFileText">Выберите файл</sys:String>
    <sys:String x:Key="BatchFileText">Пакетный файл</sys:String>
